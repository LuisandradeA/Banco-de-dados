@{
    ViewData["Title"] = "Empregado";
    @model List<Empregado>
}

<div class="text-center">
    <h1 class="display-4">Empregados</h1>
</div>

<div>
  <table class="table">
        <tr>
          <th>Nome</th>
          <th>Nome do Meio</th>
          <th>Sobrenome</th>
          <th>Codigo</th>
          <th>Data de Nascimento</th>
          <th>Endereco</th>
          <th>Sexo</th>
          <th>Salario</th>
          <th>gerente</th>
          <th>Departamento</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Nome </td>
                <td>@item.NomeDoMeio </td>
                <td>@item.SobreNome </td>
                <td>@item.Codigo </td>
                <td>@item.Dtnascimento </td>
                <td>@item.Endereco </td>
                <td>@item.Sexo </td>
                <td>@item.Salario </td>
                <td>@item.Gerente </td>
                <td>@item.Departamento </td>
            </tr>
        }
        
  
  </table>
</div>


